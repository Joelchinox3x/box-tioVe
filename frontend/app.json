{
  "expo": {
    "name": "El Ring de los Campeones",
    "slug": "boxevent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "url": "https://u.expo.dev/4b81be8b-54e1-40ec-a16f-42e3a97d85a7"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boxevent.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.boxevent.app",
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4b81be8b-54e1-40ec-a16f-42e3a97d85a7"
      }
    },
    "owner": "joelchino",
    "plugins": [
      "expo-audio",
      [
        "expo-image-picker",
        {
          "photosPermission": "Necesitamos acceso a tus fotos para que puedas subir tu perfil de peleador.",
          "cameraPermission": "Necesitamos acceso a tu c√°mara para tomar tu foto de perfil de peleador."
        }
      ],
      "expo-font",
      "expo-asset"
    ]
  }
}
